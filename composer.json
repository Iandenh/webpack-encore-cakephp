{
  "name": "iandenh/webpack-encore-cakephp",
  "description": "CakePHP 3 integration with Webpack Encore",
  "type": "cakephp-plugin",
  "license": "MIT",
  "keywords": [
    "webpack",
    "encore",
    "symfony/webpack-encore",
    "cakephp",
    "cakephp3"
  ],
  "authors": [
    {
      "name": "Ian den Hartog",
      "email": "iandenhartog@hotmail.com",
      "homepage": "https://iandh.nl/"
    }
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "cakephp/cakephp": "^3.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7.14|^6.0",
    "cakephp/cakephp-codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Iandenh\\WebpackEncore\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Iandenh\\WebpackEncore\\": "tests/"
    }
  }
}